<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="paperformat_label_sheet_eminis" model="report.paperformat">
            <field name="name">E-minis Label Sheet</field>
            <field name="default" eval="True" />
            <field name="format">custom</field>
            <field name="page_height">55</field>
            <field name="page_width">37</field>
            <field name="orientation">Landscape</field>
            <field name="margin_top">0</field>
            <field name="margin_bottom">0</field>
            <field name="margin_left">0</field>
            <field name="margin_right">0</field>
            <field name="disable_shrinking" eval="True"/>
            <field name="dpi">96</field>
        </record>

        <record id="report_product_template_label_eminis" model="ir.actions.report">
            <field name="name">Product Label (PDF)</field>
            <field name="model">product.template</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">gst_eminis_product.report_producttemplatelabel_eminis</field>
            <field name="report_file">gst_eminis_product.report_producttemplatelabel_eminis</field>
            <field name="paperformat_id" ref="gst_eminis_product.paperformat_label_sheet_eminis"/>
            <field name="print_report_name">'Products Labels - %s' % (object.name)</field>
            <field name="binding_type">report</field>
        </record>
    </data>
</odoo>
